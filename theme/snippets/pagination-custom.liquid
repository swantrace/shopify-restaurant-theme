<nav>
  <ul class="pagination justify-content-center">
    {% if paginate.previous %}
      <li class="page-item">
        <a class="page-link" href="{{ paginate.previous.url }}" title="{{ paginate.previous.title }}">
          <span>&laquo;</span>
        </a>
      </li>
    {% else %}
      <li class="page-item disabled">
        <span class="page-link">
          <span>&laquo;</span>
        </span>
      </li>
    {% endif %}
  
    {% for part in paginate.parts %}
      {% if part.is_link %}
        <li class="page-item">
          <a href="{{ part.url }}" title="" class="page-link">{{ part.title }}</a>
        </li>
      {% else %}
        {% if part.title == paginate.current_page %}
          <li class="page-item active"><span class="page-link">{{ part.title }}</span></li>
        {% else %}
          <li class="page-item"><span class="page-link">{{ part.title }}</span></li>
        {% endif %}
      {% endif %}
    {% endfor %}
  
    {% if paginate.next %}
      <li class="page-item">
        <a href="{{ paginate.next.url }}" title="{{ paginate.next.title }}" class="page-link">
          <span>&raquo;</span>
        </a>
      </li>
    {% else %}
      <li class="page-item disabled">
        <span class="page-link">&raquo;</span>
      </li>
    {% endif %}
  </ul>
</nav>
